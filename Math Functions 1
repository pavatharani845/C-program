#include <stdio.h>

#include <math.h>

int main() {

    float f;

    int num, base, exponent;

    // Input values

    scanf("%f", &f);

    scanf("%d", &num);

    scanf("%d", &base);

    scanf("%d", &exponent);

    // Output the math function results

    printf("%.0f\n", floor(f));          // Floor of float

    printf("%.0f\n", ceil(f));           // Ceil of float

    printf("%.0f\n", sqrt(num));         // Square root of integer

    printf("%.0f\n", pow(base, exponent)); // Power calculation

    return 0;

}
