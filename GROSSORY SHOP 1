#include <stdio.h>

int main() {

    int a4sheet, pen, pencil, eraser;

    // Taking input for each item

    scanf("%d", &a4sheet);

    scanf("%d", &pen);

    scanf("%d", &pencil);

    scanf("%d", &eraser);

    // Printing itemized price list

    printf("Items Details\n");

    printf("A4sheet:%d\n", a4sheet);

    printf("Pen:%d\n", pen);

    printf("Pencil:%d\n", pencil);

    printf("Eraser:%d\n", eraser);

    return 0;

}
