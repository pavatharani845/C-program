#include <stdio.h>

int main() {
    int n, i, age;
    int youngest = 1000, oldest = -1; 
    int valid = 1;

    scanf("%d", &n);

    if (n <= 0) {
        printf("Invalid Input\n");
        return 0;
    }

    for (i = 0; i < n; i++) {
        scanf("%d", &age);
        
        if (age < 0) {
            valid = 0;
            break;
        }

        if (age < youngest)
            youngest = age;

        if (age > oldest)
            oldest = age;
    }

    if (!valid) {
        printf("Invalid Input\n");
    } else {
        printf("Youngest=%d\n", youngest);
        printf("Oldest=%d\n", oldest);
    }

    return 0;
}
